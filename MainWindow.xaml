<Window x:Class="RogueFeature.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Name="winMain">
    <Grid KeyDown="Grid_KeyDown">
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DockPanel Name="spLevel" Grid.Row="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
        <StackPanel Name="spMain" Grid.Row="1"/>
    </Grid>
</Window>
